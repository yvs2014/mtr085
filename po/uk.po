## cmd help

msgid "Commands"
msgstr "Команди"

msgid "set bit pattern in range 0..255 (negative value means random)"
msgstr "встановити бітовий шаблон у діапазоні 0..255 (випадковий якщо значення від'ємне)"

msgid "set number of cycles to run (no limit: 0)"
msgstr "встановити кількість циклів виконання (без обмежень: 0)"

msgid "switch display mode"
msgstr "перемикання режиму відображення"

msgid "toggle MPLS info"
msgstr "вкл/викл MPLS відображення"

msgid "set first TTL (default 1)"
msgstr "встановити початковий TTL (стандартно 1)"

msgid "set interval in seconds (default 1s)"
msgstr "встановити інтервал у секундах (стандартно 1сек)"

msgid "toggle latency/jitter stats (default: latency)"
msgstr "перемикання відображення типу статистики, затримка чи джиттер (стандартно: затримка)"

msgid "toggle ASN lookup"
msgstr "вкл/викл відображення АС"

msgid "switch IP info"
msgstr "перемикання режимів IP-інфо"

msgid "set max TTL (default 30)"
msgstr "встановити максимум для TTL (стандартно 30)"

msgid "toggle DNS"
msgstr "вкл/викл DNS"

msgid "set stat fields to display (default: LS_NABWV)"
msgstr "встановити поля статистики у відображенні (стандартно: LS_NABWV)"

msgid "quit"
msgstr "вихід"

msgid "set ToS/QoS (quality of service)"
msgstr "встановити ToS/QoS (якість сервісу)"

msgid "reset statistics"
msgstr "обнулити статистику"

msgid "set payload size (default 56), randomly within size range if it's negative"
msgstr "встановити розмір данних (стандартно 56), випадково у діапазоні розміру якщо розмір від’ємний"

msgid "toggle TCP pings"
