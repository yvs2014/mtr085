## cmd help

msgid "Commands"
msgstr "Comandi"

msgid "set bit pattern in range 0..255 (negative value means random)"
msgstr "imposta il modello di bit nell'intervallo 0..255 (valore negativo per casuale)"

msgid "set number of cycles to run (no limit: 0)"
msgstr "imposta il numero di cicli da eseguire (illimitato: 0)"

msgid "switch display mode"
msgstr "cambia modo di visualizzazione"

msgid "toggle MPLS info"
msgstr "(dis)attiva info MPLS"

msgid "set first TTL (default 1)"
msgstr "imposta il TTL minimo (predefinito: 1)"

msgid "set interval in seconds (default 1s)"
msgstr "imposta l'intervallo in secondi (predefinito: 1sec)"

msgid "toggle latency/jitter stats (default: latency)"
msgstr "alterna latenza/jitter (predefinito: latenza)"

msgid "toggle ASN lookup"
msgstr "(dis)attiva ricerca SA"

msgid "switch IP info"
msgstr "(dis)attiva info IP"

msgid "set max TTL (default 30)"
msgstr "imposta il TTL massimo (predefinito: 30)"

msgid "toggle DNS"
msgstr "(dis)attiva DNS"

msgid "set stat fields to display (default: LS_NABWV)"
msgstr "imposta i campi delle statistiche (predefinito: LS_NABWV)"

msgid "quit"
msgstr "uscita"

msgid "set ToS/QoS (quality of service)"
msgstr "imposta ToS/QoS (qualità del servizio)"

msgid "reset statistics"
msgstr "ripristina statistiche"

msgid "set payload size (default 56), randomly within size range if it's negative"
msgstr "imposta la dimensione dei dati (predefinita: 56), casuale se negativa"

msgid "toggle TCP pings"
msgstr "(dis)attiva ping TCP"

msgid "toggle UDP pings"
msgstr "(dis)attiva ping UDP"

msgid "toggle cache mode"
msgstr "(dis)attiva modo cache"

